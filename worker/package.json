{
  "name": "zanes-crm-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute zanes-crm-db --local --file=./src/db/schema.sql",
